{
  "name": "astro-click-to-source",
  "version": "0.1.4",
  "description": "Astro integration for Alt+Click to open source files in your editor during development",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "astro-integration",
    "withastro",
    "astro",
    "dev-tools",
    "developer-experience",
    "click-to-source",
    "open-in-editor"
  ],
  "author": "invisible1988",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/invisible1988/astro-click-to-source.git"
  },
  "homepage": "https://github.com/invisible1988/astro-click-to-source#readme",
  "bugs": {
    "url": "https://github.com/invisible1988/astro-click-to-source/issues"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "format": "biome format --write",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "astro": "^4.0.0 || ^5.0.0"
  },
  "dependencies": {
    "launch-editor": "^2.12.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "astro": "^5.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vite": "^6.0.0"
  }
}
